<h1> Student Form </h1>
<%= form_tag students_path do %>
    <label>First Name:</label>
    <input type="text" id="first_name" name="students[first_name]">

    <label>Last Name:</label>
    <input type="text" id="last_name" name="students[last_name]">


    <%= submit_tag "Submit Student" %>
<% end %>